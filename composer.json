{
    "name": "robbielove/laravel-lobster-name",
    "description": "A simple package to add traits to generate names",
    "type": "package",
    "license": "MIT",
    "keywords": [
        "laravel"
    ],
    "authors": [
        {
            "name": "Robbie Love",
            "email": "13571547+robbielove@users.noreply.github.com"
        }
    ],
    "require": {
        "illuminate/support": "^7.0|^8.0"
    },
    "autoload": {
        "psr-4": {
            "Robbielove\\LobsterName\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Robbielove\\LobsterName\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Robbielove\\LobsterName\\LobsterNameServiceProvider"
            ]
        }
    },
    "require-dev": {
        "orchestra/testbench": "^6.0"
    }
}
